import java.util.Scanner;
                                             
public class SampleClass{
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int ar =sc.nextInt();
		
		int arr[]=new int[ar];
		
		System.out.println("enter a number");
		for (int i = 0;i<arr.length;i++) {
            
            arr[i]=sc.nextInt();
            
        }
		for(int item: arr)
		System.out.print(item);
		
		
        }
	}
