import java.util.Scanner;

public class prime {
    
    public static void main (String[] args)
    {
       
        
        Scanner sc = new Scanner(System.in);
        System.out.println("Number: ");
      int num = sc.nextInt();
      
        for(int i = 1; i<=9; i++){
        if (num % i == 0){
            System.out.println("it is not prime no.");
        } 
        
        else {
            System.out.println("it is  prime no.");
        } 
        
        }
      
        //  System.out.println("it is  prime no.");
             
        
    }
    
}

