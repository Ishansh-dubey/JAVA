package stringDemo;

import java.util.Scanner;

class Temp{
	int a;
	public static int b;
	
	public Temp() {
		b++;
	}
	
	public static void fun() {
		System.out.println("sdfs");
	}
}

class Temp2{
	public static void fun() {
		int a = 5;
		System.out.println(a);
	}
}


public class Live {
	
public static void main(String[] args) {
		
		Temp obj1 = null;
		
		obj1.a = 5;
		
	}
}
