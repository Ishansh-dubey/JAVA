import java.util.Scanner;

public class prime {
    
    public static void main (String[] args)
    {
       
        
        Scanner sc = new Scanner(System.in);
        System.out.println("Number: ");
      int num = sc.nextInt();
      
        for(int i = 2; i<num; i++){
        if(num % i == 0){
            System.out.println("it is prime no.");
        } 
      else {
          System.out.println("it is not prime no.");
      }       
        }
    }
    
}



