public class dice {
public static void main(String[] args) {
    
    
   int a = (int) (6*Math.random()+1);
   System.out.println("a: " + a);
   
    int b = (int) (6*Math.random()+1);
    System.out.println("b: " + b);
    
    
    int sum = a + b ;
    
    System.out.println("sum: " + sum);
    
    
    
}
}
   
