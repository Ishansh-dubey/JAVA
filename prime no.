import java.util.Scanner;

public class prime {
    
    public static void main (String[] args)
    {
        int i = 2;
        
        Scanner sc = new Scanner(System.in);
        System.out.println("Number: ");
      int num = sc.nextInt();
        
        if(num / i == 1){
            System.out.println("it is prime no.");
        } 
      else {
          System.out.println("it is not prime no.");
      }       
    }
    
}










