import java.util.Scanner;

public class Alldivisors {
   public static void main(String args[]) {
       
      int a = 1;
       
      Scanner sc = new Scanner(System.in);
      System.out.println("Enter required number :");
      int num = sc.nextInt();
     
      for(int i = 1; i<num; i++) {
         if(num % i == 0) {
          
            
            a=a+1;
         }
          
       
      }
 
      System.out.println("Total :"+a);
}
}
