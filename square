package stringDemo;

class Square {
    
    int side = 4;
    
    float area;
    float perimeter;
    
    float area() {
        
       area= side * side;
       return area;
        
    }
    
    //void perimeter
    
}    

public class Practice2{

	
	public static void main(String[] args) {
		Square areas = new Square();
