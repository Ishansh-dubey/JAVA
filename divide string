package SampleProject;

public class SampleClass{
	
	public static void main(String[] args) {
		
		String sc = "hi my name is ishansh";
		
		int length = sc.length();
		
		int a = length / 2;
		
		System.out.println(sc.substring(0,a));
		System.out.println(sc.substring(a));
		
	}
	
}









package SampleProject;

import java.util.Scanner;

public class SampleClass{
	

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int part = sc.nextInt();
		
		String str = "ishans";
		
		int length = str.length();
		
		int d =length/part;
		
		int count = 0;
		int a = 1;
		int b = a+d;
		int i= 0;
		int e = 0;
	
		for(; i<a;  ) {
			for( e =i+d; e<b;) {
				
				
				System.out.println(str.substring(i,e));
				
				count++;
				e++;
				
			}
			if(count == part) {
				break;
			}
			else {
				i=e-1;
				a=a+d;
				b=b+d;
				
			}
			
		}

	}

	
}








package SampleProject;

import java.util.Scanner;

public class SampleClass{
	

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int part = sc.nextInt();
		
		String str = "ishansh";
		
		int length = str.length();
		int d =length/part;
	
		
		
		
		int count = 0;
		int a = 1;
		int b = a+d;
		int i= 0;
		int e = 0;
		


		
		for(; i<a;  ) {
			for( e =i+d; e<b;) {
				
				
				System.out.println(str.substring(i,e));
				
				count++;
				e++;
				
			}
			if(count == part) {
				e--;
				System.out.println(str.substring(e));
				break;
			}
			else {
				i=e-1;
				a=a+d;
				b=b+d;
				
			}
			
		  } 
		
		}
	

	}

	

	
	

	

